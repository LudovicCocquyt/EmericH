{% for StaticContent in StaticContents|slice(7) %}
    {% if StaticContent.content == "oui" %}

    <div id="contact">
        <div class="container">
        	<!--Section: Contact v.2-->
            <section class="mb-4">

                <!--Section heading-->
                <h2 class="h1-responsive smallFont text-center text-white pt-4 my-4">Contact</h2>
                <!--Section description-->
                <p class="text-center text-white w-responsive mx-auto mb-5 smallFont">Vous avez des questions ? N'hésitez pas à me contacter.</p>

                <div class="row">

                    <!--Grid column-->
                    <div class="col-md-12 mb-md-0 mb-5">
                        <form id="contact-form" name="contact-form" method="POST">

                            <!--Grid row-->
                            <div class="row">

                                <!--Grid column-->
                                <div class="col-md-6">
                                    <div class="md-form mb-0">
                                        <input type="text" required="" id="name" name="name" class="form-control">
                                        <label class="text-white smallFont" for="name">Nom<span> *</span></label>
                                    </div>
                                </div>
                                <!--Grid column-->

                                <!--Grid column-->
                                <div class="col-md-6">
                                    <div class="md-form mb-0">
                                        <input type="text" id="email" name="email" class="form-control">
                                        <label class="text-white smallFont" for="email" class="">Votre email<span> *</span></label>
                                    </div>
                                </div>
                                <!--Grid column-->

                            </div>
                            <!--Grid row-->

                            <!--Grid row-->
                            <div class="row">
                                <div class="col-md-12">
                                    <div class="md-form mb-0">
                                        <input type="text" id="subject" name="subject" class="form-control">
                                        <label class="text-white smallFont" for="subject" class="">Projet<span> *</span></label>
                                    </div>
                                </div>
                            </div>
                            <!--Grid row-->

                            <!--Grid row-->
                            <div class="row">

                                <!--Grid column-->
                                <div class="col-md-12">

                                    <div class="md-form">
                                        <textarea type="text" id="message" name="message" rows="2" class="form-control md-textarea"></textarea>
                                        <label class="text-white smallFont" for="message">Votre message<span> *</span></label>
                                    </div>

                                </div>
                            </div>
                            <!--Grid row-->

                        </form>

                        <div class="text-center mt-1">
                            <a class="btn smallFont borderSubmit" onclick="document.getElementById('contact-form').submit();">Envoyer</a>
                        </div>
                        <div class="status"></div>
                    </div>
                    <!--Grid column-->
                </div>

            </section>
            <!--Section: Contact v.2-->
        </div>
    </div>
    {% endif %}
{% endfor %}