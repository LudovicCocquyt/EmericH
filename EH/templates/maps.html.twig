<div id="maps" class="row" style="height: 21em;">
	<div class="row pt-4 pb-4" style="justify-content: space-between;
    margin: auto;">
		<!--Google map-->
		<div class="col-md-8" id="map-container-google-1">
		  <iframe src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d2546.6910821867273!2d3.5016645157272066!3d50.33501437945947!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x47c2ed00ef2c147d%3A0x3efbec32d3410730!2s23%20Rue%20Gustave%20Delory%2C%2059125%20Trith-Saint-L%C3%A9ger!5e0!3m2!1sfr!2sfr!4v1592496177330!5m2!1sfr!2sfr" width="400" height="300" frameborder="0" style="border:0;" allowfullscreen="" aria-hidden="false" tabindex="0"></iframe>
		<!--Google Maps-->
		</div>

	    <!--Grid column-->
        <div class="col-md-4 text-center">
            <ul class="list-unstyled mb-0">
                <li><i class="fas fa-map-marker-alt fa-2x"></i>
                    {% for StaticContent in StaticContents|slice(3, 1) %}
                        <p>{{ StaticContent.content }}</p>
                    {% endfor %}
                </li>

                <li><i class="fas fa-phone mt-4 fa-2x"></i>
                    {% for StaticContent in StaticContents|slice(4, 1) %}
                        <p>{{ StaticContent.content }}</p>
                    {% endfor %}
                </li>

                <li><i class="fas fa-envelope mt-4 fa-2x"></i>
                    {% for StaticContent in StaticContents|slice(5, 1) %}
                        <p>{{ StaticContent.content }}</p>
                    {% endfor %}
                </li>
            </ul>
        </div>
        <!--Grid column-->
    </div>
</div>